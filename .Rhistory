## Authors: Victoria Van Mierlo & Annabelle Eng
### Date: last updated: Nov 17th, 2025
#### 0. Packages & Data ####
# load required packages
library(tidyverse)
library(ggpubr)
library(report)
library(lme4)
library(MuMIn)
library(rstatix)
library(car)
library(gridExtra)
library(lmerTest) # adds on p-value calculations to LMM summary outputs.
# read in data and specify variable types
ct = read_csv("D:/McGill PhD Working Directory/Documents/Office Work/Analysis/CTmax/VirLimCTmaxdata_nov17_2025Up.csv")
ct$species <- recode_factor(ct$species,
`F. Limosus` = "F.limosus")
ct$species = as.factor(ct$species)
ct$acclim_temp = as.factor(ct$acclim_temp)
ct$ag_temp = as.numeric(ct$ag_temp)
ct$loe_temp = as.numeric(ct$loe_temp)
# create temp-to variables
ct$TempToAgitation<- ((ct$ag_temp)-(ct$initial_temp))
ct$TempToCTMax<- ((ct$loe_temp)-(ct$ag_temp))
ct$unique_trial <- factor(paste(ct$trial_num,"_",ct$species,"_",ct$acclim_temp))
ct = ct[-57,]
#### 1. Basic Data Cleaning ####
## check for statistical outliers
# calculate mean stats by species (only treatment at this point)
ct %>%
group_by(species,acclim_temp) %>%
get_summary_stats(ag_temp, type = "mean_sd") # good check
ct %>%
group_by(species,acclim_temp) %>%
get_summary_stats(loe_temp, type = "mean_sd") # good check
# Identify statistical outliers
out1 = ct %>%
group_by(species,acclim_temp) %>%
identify_outliers(ag_temp) # no outliers.
out1
out2 = ct %>%
group_by(species,acclim_temp) %>%
identify_outliers(loe_temp) # 2 outliers -> MA293-3 & MB370-2
# MA293-3 - notes where that individual was shaking/twitching around 34 C
# MB370-2 - no notes. Possibly outlier b/c of low sample size. Retain.
out2
## Remove reasoned outliers (if required).
#ct = subset(ct, indiv_ID != "C-Leg-16")
# calculate mean stats by treatment ( same as above)
ct %>%
group_by(species,acclim_temp) %>%
get_summary_stats(ag_temp, type = "mean_sd") # good check
ct %>%
group_by(species,acclim_temp) %>%
get_summary_stats(loe_temp, type = "mean_sd") # good check
## check normality by treatment
# Ag_temp
ct %>%
group_by(species,acclim_temp) %>%
shapiro_test(ag_temp) # insignif = normal - good check.
ggqqplot(ct, x = "ag_temp", facet.by = "species") #  within CIs = normal - good check.
ggqqplot(ct, x = "ag_temp", facet.by = "acclim_temp") # 18 has some strange points near the bottom - but still normal shapiro, so fine.
#LOE_temp
ct %>%
group_by(species, acclim_temp) %>%
shapiro_test(loe_temp) # insignif = normal - good check.
ggqqplot(ct, x = "loe_temp", facet.by = "species") # good check
ggqqplot(ct, x = "loe_temp", facet.by = "acclim_temp") # good check
# body size
ct%>%
group_by(species, acclim_temp) %>%
shapiro_test(CL_mm) # insignif = normal - good check.
ggqqplot(ct, x = "CL_mm", facet.by = "species") #  within CIs = normal - good check.
ggqqplot(ct, x = "CL_mm", facet.by = "acclim_temp")
# doesn't look like body size has any measurable affect.
#### 2. Plot ####
